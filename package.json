    "firebase": "^8.3.0",
    "history": "4.10.1",
    "immer": "^7.0.7",
    "json-server": "^0.16.1",
    "less": "^3.12.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-apexcharts": "^1.3.7",
    "react-dom": "^16.13.1",
    "react-dotenv": "^0.1.3",
    "react-icons": "^3.10.0",
    "react-loading": "^2.0.3",
    "react-redux": "^7.2.1",
